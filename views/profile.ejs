<div class="page-header">

    <h3><%= user.local.name || user.facebook.name %>'s GiftBot Account</h3>

    <div class="row">
        <div class="col s12">
          <ul class="tabs">
            <li class="tab col s2"><a class="active" href="#test1">Profile</a></li>
            <li class="tab col s2"><a href="#test2">Billing</a></li>
            <li class="tab col s2"><a href="#test3">Purchases</a></li>
            <li class="tab col s2 disabled"><a href="#test4"></a></li>
            <li class="tab col s2 disabled"><a href="#test5"></a></li>
            <li class="tab col s2 disabled"><a href="#test6"></a></li>
          </ul>
        </div>
        <div id="test1" class="col s12">
            <!-- <h3>Profile</h3> -->
          <div class="row">
              <form class="col s12">
                <div class="row">
                  <div class="input-field col s6">
                    <i class="material-icons prefix">account_circle</i>
                    <input id="icon_prefix" type="text" class="validate">
                    <label for="icon_prefix">Name</label>
                  </div>
                  <div class="input-field col s6">
                    <i class="material-icons prefix">phone</i>
                    <input id="icon_telephone" type="tel" class="validate">
                    <label for="icon_telephone">Telephone</label>
                  </div>
                  <div class="input-field col s12">
                    <i class="material-icons prefix">home</i>
                    <input id="icon_address" type="tel" class="validate">
                    <label for="icon_address">Address</label>
                  </div>
                  <div class="input-field col s12">
                    <i class="material-icons prefix">email</i>
                    <input id="email" type="email" class="validate">
                    <label for="email" data-error="wrong" data-success="right">Email</label>
                  </div>
                  <div class="input-field col s12">
                    <i class="material-icons prefix">lock</i>
                    <input id="password" type="password" class="validate">
                    <label for="password">Password</label>
                  </div>
                </div>
              </form>
            </div>
          </div>
        <div id="test2" class="col s12">
          <!-- <h3>Billing</h3> -->
          <div class="row">
              <form class="col s12">
                <div class="row">
                  <div class="input-field col s6">
                    <i class="material-icons prefix">credit_card</i>
                    <input id="icon_prefix" type="text" class="validate">
                    <label for="icon_prefix">Card Number</label>
                  </div>
                  <div class="input-field col s4">
                    <i class="material-icons prefix">date</i>
                    <input id="icon_telephone" type="tel" class="validate">
                    <label for="icon_telephone">Expiration</label>
                  </div>
                  <div class="input-field col s2">
                    <i class="material-icons prefix">cvv</i>
                    <input id="icon_telephone" type="tel" class="validate">
                    <label for="icon_telephone">CVV</label>
                  </div>
                  <div class="input-field col s12">
                    <i class="material-icons prefix">account_circle</i>
                    <input id="icon_prefix" type="text" class="validate">
                    <label for="icon_prefix">Name</label>
                  </div>
                  <div class="input-field col s12">
                    <i class="material-icons prefix">home</i>
                    <input id="icon_address" type="tel" class="validate">
                    <label for="icon_address">Address</label>
                  </div>
                  <p style="text-align: center">
                      <a href="#" class="btn btn-flat white modal-close">Cancel</a>
                      <a href="#" class="waves-effect waves" type="submit" name="action">Submit</a>
                  </p>
                </div>
              </form>
            </div>
        </div>
        <div id="test3" class="col s12">Purchases
        </div>
        <!-- <div id="test4" class="col s12">Test 4</div> -->
      </div>

    <a href="/logout" class="btn btn-default btn-sm">Logout</a>

</div>
<script type="text/javascript">
  var user = <%-JSON.stringify(user)%>
</script>
